{
    "AWSEBDockerrunversion": 2,
    "containerDefinitions": [
        {
            "name": "client",
            "image": "elie91/multi-client",
            "hostname": "client",
            "essential": false
        },
        {
            "name": "api",
            "image": "elie91/multi-api",
            "hostname": "api",
            "essential": false
        },
        {
            "name": "worker",
            "image": "elie91/multi-worker",
            "hostname": "worker",
            "essential": false
        },
        {
            "name": "nginx",
            "image": "elie91/multi-nginx",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80
                }
            ],
            "links": ["client", "api"]
            
        }
    ]
}